  <%= csrf_meta_tag %>
  <%= render 'layouts/stylesheets' %>
                                                                                      
</head>
<body>
    
    <div class="otstup">
    </div>
    
    <div id="main">
		<div id="main_content">
		
    <div class="container" id="content">
		
